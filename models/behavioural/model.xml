<xmodel version="2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation='http://flame.ac.uk/schema/xmml_v2.xsd'>
  <name>Firm model</name>
  <version>1</version>
  <description>An agent-based behavioural model of a firm, with agents representing markets operated in by the firm</description>

  <environment>

    <constants>
      <variable>
        <type>int</type>
        <name>marketing_factor_count</name>
        <description>The number of marketing factors</description>
      </variable>
    </constants>

    <functionFiles>
      <file>ManagementDivision_functions.c</file>
      <file>ProductionDivision_functions.c</file>
      <file>SalesAndMarketingDivision_functions.c</file>
      <file>SupplyDivision_functions.c</file>
      <file>WarehouseDivision_functions.c</file>
      <file>SaleMarket_functions.c</file>
      <file>PurchaseMarket_functions.c</file>
    </functionFiles>

    <timeUnits>
      <timeUnit>
        <name>daily</name>
        <unit>iteration</unit>
        <period>1</period>
      </timeUnit>
      <timeUnit>
        <name>weekly</name>
        <unit>daily</unit>
        <period>5</period>
      </timeUnit>
      <timeUnit>
        <name>monthly</name>
        <unit>weekly</unit>
        <period>4</period>
      </timeUnit>
    </timeUnits>

    <dataTypes>

      <dataType>
        <name>division_production_report</name>
        <description></description>
        <variables>
          <variable><type>int</type><name>division_id</name></variable>
          <variable><type>double</type><name>cumulative_costs</name></variable>
          <variable><type>double</type><name>suggested_funding</name></variable>
        </variables>
      </dataType>

      <dataType>
        <name>division_marketing_factor</name>
        <description></description>
        <variables>
          <variable><type>double</type><name>cost</name></variable>
          <variable><type>double</type><name>available</name></variable>
        </variables>
      </dataType>

      <dataType>
        <name>division_sales_history</name>
        <description></description>
        <variables>
          <variable><type>double</type><name>proportion_sales_change</name></variable>
          <variable><type>double</type><name>factor_proportions[2]</name></variable>
        </variables>
      </dataType>

      <dataType>
        <name>market_participant</name>
        <description></description>
        <variables>
          <variable><type>double</type><name>market_preference</name></variable>
          <variable><type>double</type><name>price</name></variable>
          <variable><type>double</type><name>quality</name></variable>
          <variable><type>double</type><name>marketing_factors[2]</name></variable>
          <variable><type>double</type><name>market_share</name></variable>
          <variable><type>int</type><name>quantity_demanded</name></variable>
        </variables>
      </dataType>

      <dataType>
        <name>single_type_record</name>
        <description></description>
        <variables>
          <variable><type>int</type><name>quantity</name></variable>
          <variable><type>int</type><name>time</name></variable>
        </variables>
      </dataType>

      <dataType>
        <name>delivery</name>
        <description></description>
        <variables>
          <variable><type>int</type><name>quantity</name></variable>
          <variable><type>int</type><name>delivery_time</name></variable>
          <variable><type>int</type><name>source_id</name></variable>
        </variables>
      </dataType>

      <dataType>
        <name>working_capital_inventory_component</name>
        <description></description>
        <variables>
          <variable><type>int</type><name>source_id</name></variable>
          <variable><type>int</type><name>quantity</name></variable>
        </variables>
      </dataType>

    </dataTypes>

  </environment>

  <agents>

    <xagent>
      <name>ManagementDivision</name>
      <description></description>
      <memory>
        <variable><type>int</type><name>firm_id</name></variable>
        <variable><type>division_production_report_array</type><name>production_reports</name></variable>

        <variable><type>double</type><name>cumulative_costs</name></variable>
        <variable><type>double</type><name>funds</name></variable>
        <variable><type>double</type><name>funding_slack</name></variable>

        <variable><type>int</type><name>cumulative_produced</name></variable>
        <variable><type>int</type><name>cumulative_sold</name></variable>
        <variable><type>int</type><name>cumulative_deficit</name></variable>
        <variable><type>int</type><name>current_stock</name></variable>

        <variable><type>double</type><name>price</name></variable>
        <variable><type>double</type><name>markup</name></variable>

        <variable><type>int</type><name>production_target</name></variable>
        <variable><type>double</type><name>production_expenditure</name></variable>
        <variable><type>double</type><name>production_funding_buffer</name></variable>
      </memory>
      <functions>
        <function>
          <name>MD_administrate</name>
          <description></description>
          <currentState>0</currentState>
          <nextState>1</nextState>
          <condition>
            <time>
              <period>monthly</period>
              <phase>1</phase>
            </time>
          </condition>
          <inputs>
            <input>
              <messageName>division_administration_report</messageName>
              <filter>
                <lhs><value>a.firm_id</value></lhs>
                <op>EQ</op>
                <rhs><value>m.firm_id</value></rhs>
              </filter>
            </input>
            <input>
              <messageName>division_inventory_report</messageName>
              <filter>
                <lhs><value>a.firm_id</value></lhs>
                <op>EQ</op>
                <rhs><value>m.firm_id</value></rhs>
              </filter>
            </input>
          </inputs>
          <outputs>
            <output>
              <messageName>division_production_instruction</messageName>
            </output>
            <output>
              <messageName>division_marketing_instruction</messageName>
            </output>
          </outputs>
        </function>
      </functions>
    </xagent>

    <xagent>
      <name>ProductionDivision</name>
      <description></description>
      <memory>
        <variable><type>int</type><name>firm_id</name></variable>
        <variable><type>int</type><name>division_id</name></variable>
        <variable><type>int</type><name>cumulative_output</name></variable>
        <variable><type>int</type><name>output_target</name></variable>
        <variable><type>double</type><name>cumulative_costs</name></variable>
        <variable><type>double</type><name>funds</name></variable>

        <variable><type>int</type><name>fixed_capital_market_id</name></variable>
        <variable><type>double</type><name>fixed_capital</name></variable>

        <variable><type>double</type><name>daily_unit_fixed_capital_output</name></variable>

        <variable><type>double</type><name>fixed_capital_requirement_slack</name></variable>
        <variable><type>int</type><name>fixed_capital_installation_time</name></variable>
        <variable><type>single_type_record_array</type><name>fixed_capital_deliveries</name></variable>
        <variable><type>double</type><name>fixed_capital_depreciation_rate</name></variable>

        <variable><type>int</type><name>labour_market_id</name></variable>
        <variable><type>double</type><name>labour_per_unit_fixed_capital</name></variable>
        <variable><type>double</type><name>labour_requirement_slack</name></variable>
        <variable><type>int</type><name>labour_employment_phase</name></variable>
        <variable><type>single_type_record_array</type><name>upcoming_labour_contracts</name></variable>
        <variable><type>single_type_record_array</type><name>existing_labour_contracts</name></variable>

        <variable><type>delivery_array</type><name>division_deliveries</name></variable>
        <variable><type>working_capital_inventory_component_array</type><name>working_capital_inventory</name></variable>

        <variable><type>int</type><name>delivers_to_id</name></variable>
        <variable><type>int</type><name>time_to_deliver</name></variable>
      </memory>
      <functions>
        <function>
          <name>PD_report</name>
          <description></description>
          <currentState>0</currentState>
          <nextState>1</nextState>
          <condition>
            <time>
              <period>monthly</period>
              <phase>1</phase>
            </time>
          </condition>
          <outputs>
            <output>
              <messageName>division_administration_report</messageName>
            </output>
          </outputs>
        </function>

        <function>
          <name>PD_process_instruction</name>
          <description></description>
          <currentState>1</currentState>
          <nextState>2</nextState>
          <condition>
            <time>
              <period>monthly</period>
              <phase>1</phase>
            </time>
          </condition>
          <inputs>
            <input>
              <messageName>division_production_instruction</messageName>
              <filter>
                <lhs>
                  <lhs><value>a.firm_id</value></lhs>
                  <op>EQ</op>
                  <rhs><value>m.firm_id</value></rhs>
                </lhs>
                <op>AND</op>
                <rhs>
                  <lhs><value>a.division_id</value></lhs>
                  <op>EQ</op>
                  <rhs><value>m.division_id</value></rhs>
                </rhs>
              </filter>
            </input>
          </inputs>
        </function>

        <function>
          <name>PD_update_fixed_capital</name>
          <description></description>
          <currentState>2</currentState>
          <nextState>3</nextState>
        </function>

        <function>
          <name>PD_update_labour</name>
          <description></description>
          <currentState>3</currentState>
          <nextState>4</nextState>
        </function>

        <function>
          <name>PD_request_fixed_capital</name>
          <description></description>
          <currentState>4</currentState>
          <nextState>5</nextState>
          <condition>
            <time>
              <period>monthly</period>
              <phase>1</phase>
            </time>
          </condition>
          <inputs>
            <input>
              <messageName>market_price</messageName>
              <filter>
                <lhs><value>a.fixed_capital_market_id</value></lhs>
                <op>EQ</op>
                <rhs><value>m.market_id</value></rhs>
              </filter>
            </input>
          </inputs>
          <outputs>
            <output>
              <messageName>market_order</messageName>
            </output>
          </outputs>
        </function>

        <function>
          <name>PD_request_labour</name>
          <description></description>
          <currentState>5</currentState>
          <nextState>6</nextState>
          <condition>
            <time>
              <period>monthly</period>
              <phase>1</phase>
            </time>
          </condition>
          <inputs>
            <input>
              <messageName>market_price</messageName>
              <filter>
                <lhs><value>a.labour_market_id</value></lhs>
                <op>EQ</op>
                <rhs><value>m.market_id</value></rhs>
              </filter>
            </input>
          </inputs>
          <outputs>
            <output>
              <messageName>market_order</messageName>
            </output>
          </outputs>
        </function>

        <function>
          <name>PD_add_fixed_capital_delivery</name>
          <description></description>
          <currentState>6</currentState>
          <nextState>7</nextState>
          <condition>
            <time>
              <period>monthly</period>
              <phase>1</phase>
            </time>
          </condition>
          <inputs>
            <input>
              <messageName>market_order_confirmation</messageName>
              <filter>
                <lhs>
                  <lhs><value>a.fixed_capital_market_id</value></lhs>
                  <op>EQ</op>
                  <rhs><value>m.market_id</value></rhs>
                </lhs>
                <op>AND</op>
                <rhs>
                  <lhs>
                    <lhs><value>a.firm_id</value></lhs>
                    <op>EQ</op>
                    <rhs><value>m.global_id</value></rhs>
                  </lhs>
                  <op>AND</op>
                  <rhs>
                    <lhs><value>a.division_id</value></lhs>
                    <op>EQ</op>
                    <rhs><value>m.internal_id</value></rhs>
                  </rhs>
                </rhs>
              </filter>
            </input>
          </inputs>
        </function>

        <function>
          <name>PD_record_labour</name>
          <description></description>
          <currentState>7</currentState>
          <nextState>8</nextState>
          <condition>
            <time>
              <period>monthly</period>
              <phase>1</phase>
            </time>
          </condition>
          <inputs>
            <input>
              <messageName>market_order_confirmation</messageName>
              <filter>
                <lhs>
                  <lhs><value>a.labour_market_id</value></lhs>
                  <op>EQ</op>
                  <rhs><value>m.market_id</value></rhs>
                </lhs>
                <op>AND</op>
                <rhs>
                  <lhs>
                    <lhs><value>a.firm_id</value></lhs>
                    <op>EQ</op>
                    <rhs><value>m.global_id</value></rhs>
                  </lhs>
                  <op>AND</op>
                  <rhs>
                    <lhs><value>a.division_id</value></lhs>
                    <op>EQ</op>
                    <rhs><value>m.internal_id</value></rhs>
                  </rhs>
                </rhs>
              </filter>
            </input>
          </inputs>
        </function>

        <function>
          <name>PD_update_deliveries</name>
          <description></description>
          <currentState>8</currentState>
          <nextState>9</nextState>
        </function>

        <function>
          <name>PD_produce</name>
          <description></description>
          <currentState>9</currentState>
          <nextState>10</nextState>
          <outputs>
            <messageName>division_delivery_notification</messageName>
          </outputs>
        </function>

        <function>
          <name>PD_add_delivery</name>
          <description></description>
          <currentState>10</currentState>
          <nextState>11</nextState>
          <inputs>
            <input>
              <messageName>division_delivery_notification</messageName>
              <filter>
                <lhs>
                  <lhs><value>a.firm_id</value></lhs>
                  <op>EQ</op>
                  <rhs><value>m.firm_id</value></rhs>
                </lhs>
                <op>AND</op>
                <rhs>
                  <lhs><value>a.division_id</value></lhs>
                  <op>EQ</op>
                  <rhs><value>m.target_division_id</value></rhs>
                </rhs>
              </filter>
            </input>
          </inputs>
        </function>

      </functions>
    </xagent>

    <xagent>
      <name>SalesAndMarketingDivision</name>
      <description></description>
      <memory>
        <variable><type>int</type><name>firm_id</name></variable>
        <variable><type>int</type><name>division_id</name></variable>
        <variable><type>double</type><name>cumulative_costs</name></variable>
        <variable><type>double</type><name>funds</name></variable>
        <variable><type>division_marketing_factor</type><name>factors[2]</name></variable>
        <variable><type>division_sales_history_array</type><name>history</name></variable>
        <variable><type>int</type><name>history_size_limit</name></variable>
        <variable><type>int</type><name>change_in_sales</name></variable>
        <variable><type>double</type><name>proportion_sales_change</name></variable>
        <variable><type>int</type><name>previous_sales</name></variable>
        <variable><type>double</type><name>factor_alpha</name></variable>
        <variable><type>double</type><name>factor_beta</name></variable>
      </memory>
      <functions>
        <function>
          <name>SMD_report</name>
          <description></description>
          <currentState>0</currentState>
          <nextState>1</nextState>
          <condition>
            <time>
              <period>monthly</period>
              <phase>1</phase>
            </time>
          </condition>
          <outputs>
            <output>
              <messageName>division_administration_report</messageName>
            </output>
          </outputs>
        </function>
        <function>
          <name>SMD_process_instruction</name>
          <description></description>
          <currentState>1</currentState>
          <nextState>2</nextState>
          <condition>
            <time>
              <period>monthly</period>
              <phase>1</phase>
            </time>
          </condition>
          <inputs>
            <input>
              <messageName>division_marketing_instruction</messageName>
              <filter>
                <lhs><value>a.firm_id</value></lhs>
                <op>EQ</op>
                <rhs><value>m.firm_id</value></rhs>
              </filter>
            </input>
          </inputs>
        </function>
        <function>
          <name>SMD_strategise</name>
          <description></description>
          <currentState>2</currentState>
          <nextState>3</nextState>
          <condition>
            <time>
              <period>monthly</period>
              <phase>1</phase>
            </time>
          </condition>
          <inputs>
            <input>
              <messageName>division_marketing_inventory_report</messageName>
              <filter>
                <lhs><value>a.firm_id</value></lhs>
                <op>EQ</op>
                <rhs><value>m.firm_id</value></rhs>
              </filter>
            </input>
          </inputs>
        </function>
      </functions>
    </xagent>

    <xagent>
      <name>SupplyDivision</name>
      <description></description>
      <memory>
        <variable><type>int</type><name>firm_id</name></variable>
        <variable><type>int</type><name>division_id</name></variable>

        <variable><type>int</type><name>source_market_id</name></variable>
        <variable><type>int</type><name>parent_division_id</name></variable>

        <variable><type>int</type><name>cumulative_output</name></variable>
        <variable><type>int</type><name>output_target</name></variable>

        <variable><type>double</type><name>cumulative_costs</name></variable>
        <variable><type>double</type><name>funds</name></variable>

        <variable><type>int</type><name>delivery_time</name></variable>
      </memory>
      <functions>

        <function>
          <name>SD_report</name>
          <description></description>
          <currentState>0</currentState>
          <nextState>1</nextState>
          <condition>
            <time>
              <period>monthly</period>
              <phase>1</phase>
            </time>
          </condition>
          <outputs>
            <output>
              <messageName>division_administration_report</messageName>
            </output>
          </outputs>
        </function>

        <function>
          <name>SD_process_instruction</name>
          <description></description>
          <currentState>1</currentState>
          <nextState>2</nextState>
          <condition>
            <time>
              <period>monthly</period>
              <phase>1</phase>
            </time>
          </condition>
          <inputs>
            <input>
              <messageName>division_production_instruction</messageName>
              <filter>
                <lhs>
                  <lhs><value>a.firm_id</value></lhs>
                  <op>EQ</op>
                  <rhs><value>m.firm_id</value></rhs>
                </lhs>
                <op>AND</op>
                <rhs>
                  <lhs><value>a.division_id</value></lhs>
                  <op>EQ</op>
                  <rhs><value>m.division_id</value></rhs>
                </rhs>
              </filter>
            </input>
          </inputs>
        </function>

        <function>
          <name>SD_order</name>
          <description></description>
          <currentState>2</currentState>
          <nextState>3</nextState>
          <inputs>
            <input>
              <messageName>market_price</messageName>
              <filter>
                <lhs><value>a.source_market_id</value></lhs>
                <op>EQ</op>
                <rhs><value>m.market_id</value></rhs>
              </filter>
            </input>
          </inputs>
          <outputs>
            <output>
              <messageName>market_order</messageName>
            </output>
          </outputs>
        </function>

        <function>
          <name>SD_process_order_confirmation</name>
          <description></description>
          <currentState>3</currentState>
          <nextState>4</nextState>
          <inputs>
            <input>
              <messageName>market_order_confirmation</messageName>
              <filter>
                <lhs>
                  <lhs><value>a.source_market_id</value></lhs>
                  <op>EQ</op>
                  <rhs><value>m.market_id</value></rhs>
                </lhs>
                <op>AND</op>
                <rhs>
                  <lhs>
                    <lhs><value>a.firm_id</value></lhs>
                    <op>EQ</op>
                    <rhs><value>m.global_id</value></rhs>
                  </lhs>
                  <op>AND</op>
                  <rhs>
                    <lhs><value>a.division_id</value></lhs>
                    <op>EQ</op>
                    <rhs><value>m.internal_id</value></rhs>
                  </rhs>
                </rhs>
              </filter>
            </input>
          </inputs>
          <outputs>
            <output>
              <messageName>division_delivery_notification</messageName>
            </output>
          </outputs>
        </function>

      </functions>
    </xagent>

    <xagent>
      <name>WarehouseDivision</name>
      <description></description>
      <memory>
        <variable><type>int</type><name>firm_id</name></variable>
        <variable><type>int</type><name>division_id</name></variable>

        <variable><type>double</type><name>cumulative_costs</name></variable>
        <variable><type>double</type><name>funds</name></variable>

        <variable><type>int</type><name>current_inventory</name></variable>
        <variable><type>int</type><name>cumulative_recieved</name></variable>
        <variable><type>int</type><name>cumulative_dispatched</name></variable>
        <variable><type>int</type><name>cumulative_deficit</name></variable>

        <variable><type>single_type_record_array</type><name>deliveries</name></variable>
      </memory>
      <functions>

        <function>
          <name>WD_report</name>
          <description></description>
          <currentState>0</currentState>
          <nextState>1</nextState>
          <condition>
            <time>
              <period>monthly</period>
              <phase>1</phase>
            </time>
          </condition>
          <outputs>
            <output>
              <messageName>division_administration_report</messageName>
            </output>
            <output>
              <messageName>division_inventory_report</messageName>
            </output>
          </outputs>
        </function>

        <function>
          <name>WD_marketing_report</name>
          <description></description>
          <currentState>1</currentState>
          <nextState>2</nextState>
          <condition>
            <time>
              <period>monthly</period>
              <phase>1</phase>
            </time>
          </condition>
          <outputs>
            <output>
              <messageName>division_marketing_inventory_report</messageName>
            </output>
          </outputs>
        </function>

        <function>
          <name>WD_update_deliveries</name>
          <description></description>
          <currentState>2</currentState>
          <nextState>3</nextState>
        </function>

        <function>
          <name>WD_add_delivery</name>
          <description></description>
          <currentState>3</currentState>
          <nextState>4</nextState>
          <inputs>
            <input>
              <messageName>division_delivery_notification</messageName>
              <filter>
                <lhs>
                  <lhs><value>a.firm_id</value></lhs>
                  <op>EQ</op>
                  <rhs><value>m.firm_id</value></rhs>
                </lhs>
                <op>AND</op>
                <rhs>
                  <lhs><value>a.division_id</value></lhs>
                  <op>EQ</op>
                  <rhs><value>m.target_division_id</value></rhs>
                </rhs>
              </filter>
            </input>
          </inputs>
        </function>

      </functions>
    </xagent>

    <xagent>
      <name>SaleMarket</name>
      <description></description>
      <memory>
        <variable><type>market_participant_array</type><name>participants</name></variable>
        <variable><type>double</type><name>marketing_factor_totals[2]</name></variable>
        <variable><type>int</type><name>overall_demand</name></variable>
        <variable><type>double</type><name>preference_total</name></variable>
        <variable><type>double</type><name>marketing_factor_weights[2]</name></variable>
      </memory>
      <functions>
        <function>
